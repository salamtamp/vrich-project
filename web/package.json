{
  "name": "vrich-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "rm -rf .next && next build && npm run copy-assets",
    "copy-assets": "cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "build:docker": "next build --experimental-build-mode=compile",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .next node_modules coverage",
    "tsc": "tsc -p tsconfig.json",
    "predev": "npx msw init public --save",
    "commit": "git-cz",
    "docker:build": "docker build -t vrich-web .",
    "docker:run": "docker run -p 3000:3000 vrich-web",
    "docker:dev": "docker build -t vrich-web . && docker run -p 3000:3000 vrich-web"
  },
  "dependencies": {
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "7.1.1",
    "@mui/material": "7.1.1",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "jwt-decode": "^4.0.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "next-runtime-env": "^3.3.0",
    "notistack": "^3.0.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.57.0",
    "sharp": "^0.34.2",
    "tailwind-merge": "^3.3.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@next/eslint-plugin-next": "^15.3.4",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.12.0",
    "@types/node": "^24.0.0",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.6",
    "@types/react-html-parser": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "beasties": "^0.3.4",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "file-loader": "^6.2.0",
    "msw": "^2.10.2",
    "open-cli": "^8.0.0",
    "postcss": "^8.5.4",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.6",
      "@types/react-dom": "19.1.6"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "react-dom"
      ]
    },
    "ignoredBuiltDependencies": [
      "@swc/core",
      "msw",
      "sharp",
      "unrs-resolver"
    ]
  },
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=8.0.0"
  }
}
